{
  "name": "nft_loteria_frontend",
  "version": "0.1.0",
  "private": true,
  "description": "NFTLoteria",
  "keywords": [
    "NFTLoteria"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prebuild": "npm run generate",
    "prune:tablas:dry": "DRY_RUN=1 tsx src/frontend/src/lib/data/prune-tablas.ts",
    "prune:tablas": "tsx src/frontend/src/lib/data/scripts/prune-tablas.ts",
    "prune:layouts:dry": "DRY_RUN=1 tsx src/frontend/src/lib/data/scripts/prune-layouts.ts",
    "prune:layouts": "tsx src/frontend/src/lib/data/scripts/prune-layouts.ts",
    "generate": "dfx generate && node ./update.types.mjs && npm run format"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@rollup/plugin-inject": "^5.0.5",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.15.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@types/node": "^20.10.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "^10.4.16",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.69.5",
    "svelte": "^5.15.0",
    "svelte-check": "^4.0.5",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2",
    "vite": "^5.4.11"
  },
  "type": "module",
  "dependencies": {
    "@dfinity/agent": "^2.3.0",
    "@dfinity/auth-client": "^2.3.0",
    "@dfinity/candid": "^2.3.0",
    "@dfinity/nns": "^7.0.1",
    "@dfinity/nns-proto": "^2.0.1",
    "@dfinity/principal": "^2.3.0",
    "@dfinity/sns": "^3.2.2",
    "@dfinity/utils": "^2.5.2",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "clsx": "^2.1.1",
    "dompurify": "^3.1.7",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "gsap": "^3.13.0",
    "lucide-svelte": "^0.544.0",
    "postcss": "^8.4.47",
    "svelte-flag-icons": "^1.0.3",
    "svelte-motion": "^0.12.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.14"
  }
}
