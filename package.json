{
  "name": "nft_loteria_frontend",
  "version": "0.1.0",
  "private": true,
  "description": "NFTLoteria",
  "keywords": [
    "NFTLoteria"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "prebuild": "npm run generate",
    "prune:tablas:dry": "DRY_RUN=1 tsx src/frontend/src/lib/data/prune-tablas.ts",
    "prune:tablas": "tsx src/frontend/src/lib/data/scripts/prune-tablas.ts",
    "prune:layouts:dry": "DRY_RUN=1 tsx src/frontend/src/lib/data/scripts/prune-layouts.ts",
    "prune:layouts": "tsx src/frontend/src/lib/data/scripts/prune-layouts.ts",
    "generate": "dfx generate && node ./update.types.mjs && npm run format"
  },
  "devDependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@rollup/plugin-inject": "^5.0.5",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "2.44.0",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@types/node": "^24.6.2",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "autoprefixer": "^10.4.21",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "sass": "^1.93.2",
    "svelte": "5.39.9",
    "svelte-check": "^4.3.2",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^3.4.18",
    "tslib": "^2.8.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "7.1.9"
  },
  "type": "module",
  "dependencies": {
    "@dfinity/agent": "^3.2.7",
    "@dfinity/auth-client": "^3.2.7",
    "@dfinity/candid": "^3.2.7",
    "@dfinity/ledger-icp": "6.0.2",
    "@dfinity/ledger-icrc": "4.0.3",
    "@dfinity/nns-proto": "^2.0.2",
    "@dfinity/principal": "^3.2.7",
    "@dfinity/utils": "3.2.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.7",
    "dotenv": "^17.2.3",
    "esbuild": "^0.25.10",
    "lucide-svelte": "^0.544.0",
    "qrcode": "^1.5.4",
    "svelte-flag-icons": "^2.0.2",
    "svelte-motion": "^0.12.2",
    "tailwind-merge": "^3.3.1"
  }
}
